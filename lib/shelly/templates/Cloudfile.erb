<%= @code_name %>:
  ruby: 1.9.2 # 1.9.2 or ree
  environment: production # RAILS_ENV
  monitoring_email:
    - <%= @email %>
  domains:
    - <%= @code_name %>.winniecloud.com
  servers:
    app1:
      size: large
      thin: 4
      # whenever: on
      # delayed_job: 1
    <%- @databases.each do |kind| -%>
    <%= kind %>:
      size: large
      database:
        - <%= kind %>
    <%- end -%>
